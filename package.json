{
  "name": "react_app_template",
  "version": "0.1.0",
  "description": "react preconfigured template for a new project developing fast starting",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui.html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "author": "impuDEV<impudev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.25.7",
    "@chromatic-com/storybook": "^3.2.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-onboarding": "^8.4.5",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/react": "^8.4.5",
    "@storybook/react-webpack5": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.4.0",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/jest": "^29.5.13",
    "@types/node": "^17.0.21",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^9.2.1",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-i18next": "^6.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-impudev-fsd": "^0.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "loki": "^0.35.1",
    "mini-css-extract-plugin": "^2.9.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.2",
    "reg-cli": "^0.18.9",
    "regenerator-runtime": "^0.13.9",
